{
  "add_on_info_and_versions": {
    "settings": {
      "analysis": {
        "normalizer": {
          "my_normalizer": {
            "type": "custom",
            "filter": ["lowercase"]
          }
        }
      }
    },
    "properties": {
      "moduleId": {
        "type": "keyword"
      },
      "modulePackage": {
        "type": "keyword"
      },
      "type": {
        "type": "keyword"
      },
      "status": {
        "type": "keyword"
      },
      "name": {
        "type": "text",
        "fields": {
          "raw": {
            "type": "keyword",
            "normalizer": "my_normalizer",
            "index": true
          }
        }
      },
      "description": {
        "type": "text"
      },
      "tags": {
        "type": "keyword"
      },
      "links": {
        "type": "nested",
        "properties": {
          "rel": {
            "type": "keyword"
          },
          "href": {
            "type": "keyword",
            "index": false
          },
          "title": {
            "type": "text"
          }
        }
      },
      "hostedUrl": {
        "type": "keyword",
        "index": false
      },
      "versions": {
        "type": "nested",
        "properties": {
          "moduleId": {
            "type": "keyword"
          },
          "modulePackage": {
            "type": "keyword"
          },
          "version": {
            "type": "keyword"
          },
          "releaseDatetime": {
            "type": "date"
          },
          "downloadUri": {
            "type": "keyword",
            "index": false
          },
          "renameTo": {
            "type": "keyword",
            "index": false
          },
          "requireOpenmrsVersion": {
            "type": "text",
            "index": false
          },
          "requireModules": {
            "type": "nested",
            "properties": {
              "module": {
                "type": "text"
              },
              "version": {
                "type": "text",
                "index": false
              }
            }
          },
          "supportedLanguages": {
            "type": "keyword"
          }
        }
      }
    }
  }
}
